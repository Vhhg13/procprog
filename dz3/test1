#!/bin/bash

tst () {
    echo
    echo $1 ' | ' $2
    echo $1 | ./z1
}
#     S  p  n
tst '120 10 1' '11'
tst '120 100 1' '20'
tst '120 50 2' '9'
tst '0 100 1' '0'
tst '0 0 0' '0'
tst '12 0 1' '1'
tst '120 0 1' '10'

echo '
##############################
# p==0                       #
##############################'

tst '120 0 1' '10'
tst '120 0.0 1' '10'
tst '240 0 2' '10'
tst '480 0 1' '40'



echo '
##############################
# !!!!!!!!!!!!!!!!!!!!!!!!!! #
##############################'


tst '120 0 0' 'p,n!=0'
tst '120 0.0 0.0' 'p,n!=0'


echo '
##############################
# n!=0                       #
##############################'

tst '120 100 0' 'n!=0'
tst '120 100 0.0' 'n!=0'
