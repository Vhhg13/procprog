#include <stdio.h>

int factorial(int num){
  int res=1;
  for(int i=1;i<=num;++i)
    res*=i;
  return res;
}

typedef int shar;

_Bool in(shar sh, shar * shary){
  for(int i=0;shary[i]!=0;++i)
    if(sh==shary[i]) return 1;
  return 0;
}

_Bool aldif(shar *shary){
  if(shary[0]==0) return 1;
  return !in(shary[0], shary+1) && aldif(shary+1);
}

_Bool fun(shar* shary){
  for(int i=0;shary[i]!=0;++i)
    if(shary[i]==i+1+48) return 1;
  return 0;
}

void permute(int m, int n){

}

int main(void){
  int n=10, z=0;
  shar shary[n];
    
  
  int result=factorial(n-1)+(n-1)*z;
  printf("%d\n", result);
  return 0;
}
